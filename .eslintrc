{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier",
  ],
  "plugins": ["@typescript-eslint"],
  "env": {
    "node": true,
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "ecmaFeatures": {
      "legacyDecorators": true,
      "jsx": true,
    },
  },
  "rules": {
    "react/jsx-handler-names": 0,
    "react/jsx-fragments": 0,
  },
}
