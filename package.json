{
  "name": "@solace-labs/maas-icons",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solacelabs/maas-icons.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "keywords": [
    "icons",
    "ux",
    "sc",
    "solace"
  ],
  "bugs": {
    "url": "https://github.com/solacelabs/maas-ux-icons/issues"
  },
  "author": "Kevin Lidstone",
  "version": "0.2.149",
  "description": "Svg icons library",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "rm -rf dist && microbundle --compress --jsx React.createElement --format cjs,modern --define process.env.NODE_ENV=production",
    "build:dev": "microbundle watch --jsx React.createElement --no-compress --format cjs --define process.env.NODE_ENV=development",
    "test": "run-s test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "copyAssets": "node ./scripts/copyAssets.js",
    "optimize": "rimraf ./optimized && svgo -r --folder ./icons -o ./optimized --pretty --indent=2",
    "generateicons": "rm -rf reactGenerated && node ./scripts/build.js",
    "optimize:illustrations": "rimraf ./optimizedIllustrations && svgo --config=svgo.config.illustrations.mjs -r --folder ./illustrations -o ./optimizedIllustrations --pretty --indent=2",
    "optimize:images": "rimraf ./optimizedImages && svgo --config=svgo.config.illustrations.mjs -r --folder ./images -o ./optimizedImages --pretty --indent=2",
    "prepublishOnly": "npm run optimize && npm run optimize:illustrations && npm run optimize:images && npm run generateicons  && npm run build && npm run copyAssets"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.12.38",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-eslint": "^10.1.0",
    "camelcase": "^6.3.0",
    "cross-env": "^7.0.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "gh-pages": "^6.1.1",
    "microbundle": "^0.15.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "react-scripts": "^5.0.0",
    "rimraf": "^5.0.5",
    "svg-sprite": "^1.5.3",
    "svgo": "^3.2.0",
    "typescript": "^4.9.5"
  },
  "files": [
    "dist",
    "dist/illustrations"
  ],
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.15.10"
  }
}
